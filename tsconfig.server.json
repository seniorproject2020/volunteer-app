{
    "compilerOptions": {
      "outDir": "./dist/",
      "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "lib": [
          "es2017"
      ],
      "types": [
          "node", "express", "mongoose"
      ]
    },
    "include": [
        "src/server/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }