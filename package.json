{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:server": "webpack --mode production --config webpack.config.server.js",
    "build:client": "webpack --mode production --config webpack.config.client.js",
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "webpack --watch --config webpack.config.client.js --mode development --devtool inline-source-map",
    "dev:server": "concurrently  \"yarn server:build\" \"yarn server:watch\"",
    "client:dev-server": "webpack-dev-server --config webpack.config.client.js --mode development --devtool inline-source-map --hot",
    "start": "yarn build && node ./dist/server.js",
    "server:watch": "nodemon --inspect=\"9229\" \"./dist/server.js\" --watch \"./dist\" ",
    "server:build": "webpack --watch --config webpack.config.server.js"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@date-io/moment": "^1.x",
    "@material-ui/core": "^4.9.3",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "mongodb-client-encryption": "^1.0.1",
    "mongoose": "^5.9.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-bootstrap-icons": "^1.0.1-alpha3",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.13.0",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}